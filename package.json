{
  "name": "ygogui",
  "version": "0.4.0",
  "description": "A developing GUI for VP9/Opus encoder",
  "repository": "https://github.com/ytingyeu/ygogui",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -w"
  },
  "build": {
    "appId": "org.ncuacg.app",
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": "ia32"
        }
      ]
    },
    "icon": "./icon.png",
    "extraResources": [
      "./tools/**"
    ]
  },
  "author": "Tingyeu Yang",
  "license": "GPLv3",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "^20.38.3"
  },
  "dependencies": {}
}
