{
  "name": "ygogui",
  "version": "0.5.0",
  "description": "A developing GUI for VP9/Opus encoder",
  "repository": "https://github.com/ytingyeu/ygogui",
  "main": "./app/main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder -w",
    "postinstall": "electron-builder install-app-deps",
    "clean": "rm -rf node_modules"
  },
  "build": {
    "appId": "org.ncuacg.app",
    "win": {
      "target": [
        {
          "target": "zip",
          "arch": "ia32"
        }
      ]
    },
    "icon": "./favicon.ico",
    "extraResources": [
      {
        "from": "./tools/",
        "to": "tools",
        "filter": [
          "!**/*",
          "ffmpeg.exe",
          "ffprobe.exe"
        ]
      }
    ]
  },
  "author": "Tingyeu Yang",
  "license": "GPL-3.0",
  "devDependencies": {
    "electron": "^3.0.7",
    "electron-builder": "^20.44.4"
  },
  "dependencies": {
    "fluent-ffmpeg": "^2.1.2",
    "mathjs": "^6.0.2"
  }
}
